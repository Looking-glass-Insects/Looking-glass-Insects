<?xml version="1.0" encoding="utf-8"?>
<com.example.heyong.exercisesbase.CustomView.RightSlideCloseFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:dslv="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/model_root">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/relativeLayout">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="模板库"
            android:textSize="40dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:id="@+id/textView3" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#dddddd"
            android:id="@+id/line1"
            android:layout_below="@+id/textView3"
            android:orientation="horizontal"></LinearLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/line1"
            android:layout_alignParentStart="true">
            <ListView
                android:id="@+id/model_lv"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                >
            </ListView>
        </RelativeLayout>
    </RelativeLayout>

</com.example.heyong.exercisesbase.CustomView.RightSlideCloseFrameLayout>